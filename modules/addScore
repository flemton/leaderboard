const userName = document.querySelector('#name');
const userScore = document.querySelector('#score');

async function addScore() {
    const response = await fetch('https://us-central1-js-capstone-backend.cloudfunctions.net/api/games/IqNrWE04pgaXdxjtzDh9/scores/', {
    method: 'POST',
    body: JSON.stringify({"user": userName.value, "score": userScore })
  });
}

export default addScore;